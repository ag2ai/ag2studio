{
  "name": "ag2studio",
  "version": "0.1.0",
  "private": true,
  "description": "AG2 Studio - Build LLM Enabled Agents",
  "author": "SPIRAL Team",
  "scripts": {
    "dev": "next dev",
    "clean": "rimraf .next out",
    "build": "npm run clean && next build && rimraf ../ag2studio/web/ui && rsync -a --delete out/ ../ag2studio/web/ui/",
    "build-windows": "npm run clean && next build && rimraf ../ag2studio/web/ui && xcopy /E /I /Y out ..\\ag2studio\\web\\ui",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.6",
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@monaco-editor/react": "^4.6.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^10.0.0",
    "antd": "^5.1.0",
    "autoprefixer": "^10.4.7",
    "jszip": "^3.10.1",
    "lodash.debounce": "^4.0.8",
    "next": "^15.1.0",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.13",
    "react": "^18.2.0",
    "react-contenteditable": "^3.3.6",
    "react-dom": "^18.2.0",
    "react-inner-image-zoom": "^3.0.2",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.7.2",
    "uuid": "^11.0.3",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.15",
    "@types/react-syntax-highlighter": "^15.5.13",
    "rimraf": "^6.0.1"
  }
}
